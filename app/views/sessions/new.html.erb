<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

<br>
<div class="login">
  <%= f.submit "Log in", class: 'btn btn-info' %>
</div>
<% end %>